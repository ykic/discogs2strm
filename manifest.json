{
  "name": "discogs2strm",
  "version": "1.1",
  "description": "Add search link to Music Streaming Service",
  "permissions": [
    "https://*.discogs.com/"
  ],
  "icons": {
    "16": "src/img/icon16.png",
    "48": "src/img/icon48.png",
    "128": "src/img/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.discogs.com/*"
      ],
      "js": [
        "src/lib/jquery-3.5.1.min.js",
        "src/js/contentScript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "src/img/*.png"
  ],
  "manifest_version": 2
}